package ex06.autowired;

public class Printer2 {

	private Document document;

	//기본생성자
	public Printer2() {}
	
	//생성자
	public Printer2(Document document) {
		this.document = document;
	}

	//게터 , 세터
	public Document getDocument() {
		return document;
	}

	public void setDocument(Document document) {
		this.document = document;
	}
	
}
